    "ng": "ng",
    "restore": "yarn add @tei/abstraction@latest @tei/admin-admin-center@latest @tei/admin-device-configuration@latest @tei/admin-translations@latest @tei/booking-activities@latest @tei/booking-dining@latest @tei/booking-dining-shared@latest @tei/booking-dining-states@latest @tei/booking-event@latest @tei/booking-lottery@latest @tei/booking-tee-times@latest @tei/booking-tee-times-shared@latest @tei/cfd@latest @tei/common-automated-notification@latest @tei/common-booking-requirements-shared@latest @tei/common-contact-lookup-shared@latest @tei/common-field-management@latest @tei/common-field-management-shared@latest @tei/common-module-settings@latest @tei/common-my-account@latest @tei/common-my-account-shared@latest @tei/common-my-account-widgets@latest @tei/common-payment-engine@latest @tei/common-payment-engine-shared@latest @tei/common-payment-processor@latest @tei/core@latest @tei/core-signalr@latest @tei/core-translate@latest @tei/common-devices@latest @tei/ecommerce-online-check-in@latest @tei/fabric-wrapper@latest @tei/formly-custom-components@latest @tei/ip-email-builder@latest @tei/material@latest @tei/material-generic-filter@latest @tei/material-html-builder@latest @tei/material-pdf-viewer@latest @tei/office-contacts@latest @tei/office-dashboard@latest @tei/office-event-management@latest @tei/pos-eod@latest @tei/pos-food-and-beverage@latest @tei/pos-food-and-beverage-admin@latest @tei/pos-retail@latest @tei/pos-retail-admin@latest @tei/pos-shared@latest @tei/pos-tee-sheet@latest @tei/retail-admin@latest @tei/retail-ecommerce@latest @tei/security-user-management@latest @tei/theme@latest @tei/material-smart-components@latest @tei/cms-admin@latest @tei/marketing-cms@latest --ignore-engines",
    "restore-beta": "yarn add @tei/abstraction@beta @tei/admin-admin-center@beta @tei/admin-device-configuration@beta @tei/admin-translations@beta @tei/booking-activities@beta @tei/booking-dining@beta @tei/booking-dining-shared@beta @tei/booking-dining-states@beta @tei/booking-event@beta @tei/booking-lottery@beta @tei/booking-tee-times@beta @tei/booking-tee-times-shared@beta @tei/cfd@beta @tei/common-automated-notification@beta @tei/common-booking-requirements-shared@beta @tei/common-contact-lookup-shared@beta @tei/common-field-management@beta @tei/common-field-management-shared@beta @tei/common-module-settings@beta @tei/common-my-account@beta @tei/common-my-account-shared@beta @tei/common-my-account-widgets@beta @tei/common-payment-engine@beta @tei/common-payment-engine-shared@beta @tei/common-payment-processor@beta @tei/core@beta @tei/core-signalr@beta @tei/core-translate@beta @tei/common-devices@beta @tei/ecommerce-online-check-in@beta @tei/fabric-wrapper@beta @tei/formly-custom-components@beta @tei/ip-email-builder@beta @tei/material@beta @tei/material-generic-filter@beta @tei/material-html-builder@beta @tei/material-pdf-viewer@beta @tei/office-contacts@beta @tei/office-dashboard@beta @tei/office-event-management@beta @tei/pos-eod@beta @tei/pos-food-and-beverage@beta @tei/pos-food-and-beverage-admin@beta @tei/pos-retail@beta @tei/pos-retail-admin@beta @tei/pos-shared@beta @tei/pos-tee-sheet@beta @tei/retail-admin@beta @tei/retail-ecommerce@beta @tei/security-user-management@beta @tei/theme@beta @tei/material-smart-components@beta @tei/cms-admin@beta @tei/marketing-cms@beta --ignore-engines",
    "conventional-changelog": "conventional-changelog",
    "start": "cross-env NG_PERSISTENT_BUILD_CACHE=1 ng serve",
    "start:host": "ng serve --host app.totaleintegrated.com --ssl --port 443",
    "start:portal-group": "ng serve --host teidev.totaleintegrated.com --ssl",
    "start:portal": "ng serve --host teiwebdev.totaleintegrated.com --port 14200",
    "start:tenants": "concurrently --kill-others \"npm run start:host\" \"npm run start:portal-group\" \"npm run start:portal\"",
    "build": "cross-env NG_PERSISTENT_BUILD_CACHE=1 ng build tei-shell --configuration=development",
    "build:app-core-lib": "ng build --project app-core-lib",
    "build:dev-all": "npm run restore && npm run build:dev && npm run serve",
    "build:dev": "ng build tei-shell --configuration=development",
    "build:prod": "ng build tei-shell --configuration=production",
    "publish:local": "npm run build:app-core-lib  && npm run bundle-assets && node ./publish-local.js",
    "publish:app-core-lib": "npm run release && npm publish ./destination/app-core-lib",
    "publish:mac-app-core-lib": "npm run release-mac && npm publish ./destination/app-core-lib",
    "bundle-assets": "XCOPY src\\localization\\shell destination\\app-core-lib\\assets\\localization /s /i /y /d",
    "bundle-assets-mac": "rsync -r src/localization destination/app-core-lib",
    "release": "npm run build:app-core-lib && npm version patch --prefix ./projects/app-core-lib && npm run build:app-core-lib && npm run bundle-assets",
    "release-mac": "npm run build:app-core-lib && npm version patch --prefix ./projects/app-core-lib && npm run build:app-core-lib && npm run bundle-assets-mac",
    "watch": "ng build --configuration=development --watch",
    "serve": "ng serve --configuration=development --disable-host-check",
    "serve:prod": "ng serve --configuration=production",
    "test": "ng test tei-shell",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint tei-shell --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "postbuild": "node ./postbuild.js",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "build:mobile": "ng build tei-shell --configuration=mobile",
    "build:mobile-dev": "ng build tei-shell --base-href / --configuration=mobile-dev",
    "cap:init": "npx cap init",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:sync:ios": "npx cap sync ios",
    "cap:sync:android": "npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:copy": "npx cap copy",
    "cap:update": "npx cap update",
    "mobile:build-sync": "npm run build:mobile && npm run cap:sync",
    "mobile:build-sync-dev": "npm run build:mobile-dev && npm run cap:sync",
    "mobile:ios": "npm run build:mobile && npm run cap:sync:ios && npm run cap:open:ios",
    "mobile:ios-dev": "npm run build:mobile-dev && npm run cap:sync:ios && npm run cap:open:ios",
    "mobile:android": "npm run build:mobile && npm run cap:sync:android && npm run cap:open:android",
    "mobile:android-dev": "npm run build:mobile-dev && npm run cap:sync:android && npm run cap:open:android"
  },
